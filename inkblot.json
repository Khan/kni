{
  "name": "inkblot",
  "description": [
    "inkblot [-j] [-s label] script.ink"
  ],
  "usage": [
    "<script> :input  Script file name",
    "[-s <start>]     Start script at this label",
    "[-j|--json]      Dump compiled script as JSON instead of running interactively.",
    "[--dot]          Dump the final parse tree in Graphviz DOT format.",
    "[--di]           Log input file before processing",
    "[--dp]           Enable parser debug logging",
    "[--dil]          Enable inline lexer debug logging",
    "[--dol]          Enable outline lexer debug logging",
    "[--ds]           Enable scanner debug loggin"
